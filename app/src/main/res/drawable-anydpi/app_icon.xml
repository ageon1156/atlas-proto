<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="24"
    android:viewportHeight="24"
    android:tint="#FFFFFF"
    android:alpha="0.8">

    <!-- Globe circle -->
    <path
        android:pathData="M12,2a4.5,4.5 0 1,0 0,9a4.5,4.5 0 1,0 0,-9z"
        android:strokeWidth="1.3"
        android:strokeColor="#ffffff"
        android:fillColor="#00000000"/>

    <!-- Globe vertical meridian -->
    <path
        android:pathData="M12,2L12,11"
        android:strokeWidth="1.3"
        android:strokeColor="#ffffff"/>

    <!-- Globe horizontal equator -->
    <path
        android:pathData="M7.5,6.5L16.5,6.5"
        android:strokeWidth="1.3"
        android:strokeColor="#ffffff"/>

    <!-- Globe left longitude curve -->
    <path
        android:pathData="M12,2Q9.5,6.5 12,11"
        android:strokeWidth="1.3"
        android:strokeColor="#ffffff"
        android:fillColor="#00000000"/>

    <!-- Globe right longitude curve -->
    <path
        android:pathData="M12,2Q14.5,6.5 12,11"
        android:strokeWidth="1.3"
        android:strokeColor="#ffffff"
        android:fillColor="#00000000"/>

    <!-- Left arm (from head side up to globe) -->
    <path
        android:pathData="M11,13L9,9.7"
        android:strokeWidth="1.3"
        android:strokeColor="#ffffff"/>

    <!-- Right arm (from head side up to globe) -->
    <path
        android:pathData="M13,13L15,9.7"
        android:strokeWidth="1.3"
        android:strokeColor="#ffffff"/>

    <!-- Head circle -->
    <path
        android:pathData="M12,12a1,1 0 1,0 0,2a1,1 0 1,0 0,-2z"
        android:strokeWidth="1.3"
        android:strokeColor="#ffffff"
        android:fillColor="#00000000"/>

    <!-- Left body rail -->
    <path
        android:pathData="M11.3,14L11.3,18.5"
        android:strokeWidth="1.3"
        android:strokeColor="#ffffff"/>

    <!-- Right body rail -->
    <path
        android:pathData="M12.7,14L12.7,18.5"
        android:strokeWidth="1.3"
        android:strokeColor="#ffffff"/>

    <!-- Left leg -->
    <path
        android:pathData="M11.3,18.5L10.5,22"
        android:strokeWidth="1.3"
        android:strokeColor="#ffffff"/>

    <!-- Right leg -->
    <path
        android:pathData="M12.7,18.5L13.5,22"
        android:strokeWidth="1.3"
        android:strokeColor="#ffffff"/>

